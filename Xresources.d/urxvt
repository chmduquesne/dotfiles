/*
 *      rxvt-unicode config
 * */
URxvt.perl-ext-common:                  default,selection,matcher,searchable-scrollback,clipboard
URxvt.saveLines:                        5000
URxvt.scrollBar:                        false
URxvt.colorUL:                          #86a2be
URxvt.urgentOnBell:                     true
URxvt.matcher.button:                   1
URxvt.url-launcher:                     x-www-browser
URxvt.keysym.Mod4-y:                    perl:clipboard:copy
URxvt.keysym.Mod4-p:                    perl:clipboard:paste
URxvt.clipboard.autocopy:               false
URxvt.clipboard.copycmd:                xclip -i -selection clipboard && xclip -o -selection clipboard | xargs -0 -I% notify-send "In clipboard" %
URxvt.clipboard.pastecmd:               xclip -o -selection clipboard
